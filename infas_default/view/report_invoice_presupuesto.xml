<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="report_invoice_presupuesto_page" inherit_id="l10n_ar.report_invoice_document" primary="True">
            <xpath expr="///main/div//div" position="after">
                <div>Holas</div>
            </xpath>
        </template>

        <!-- Template -->
        <template id="report_invoice_presupuesto">
            <t t-call="web.html_container">
                <t t-as="o" t-foreach="docs">
                    <div t-att-data-oe-lang="o and o.env.context.get('lang')">
                        <t t-call="infas_default.report_invoice_presupuesto_page" />
                    </div>
                </t>
            </t>
        </template>

        <!-- Add report -->
        <report id="infas_report_invoice_presupuesto" model="account.move" name="infas_default.report_invoice_presupuesto" report_type="qweb-html" string="Presupuesto" />

    </data>
</odoo>
        