<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<!--Main MRP Production FORM view-->
	<record id="custom_mrp_production_form_view" model="ir.ui.view">
        <field name="name">custom_mrp_production_form_view</field>
		<field name="model">mrp.production</field>
        <field name="inherit_id" ref="mrp.mrp_production_form_view"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='move_raw_ids']//tree//field[@name='product_id']" position="after">
                <field name="tecnical_data"/>
            </xpath>
		</field>
	</record>
</odoo>
